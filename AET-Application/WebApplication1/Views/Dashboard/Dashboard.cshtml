@{
    ViewBag.Title = "Dashboard";
}
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard</title>
    <link href="~/Content/dashboard-styling.css" rel="stylesheet" />
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/_all-skins.min.css" rel="stylesheet" />

</head>

<body>

    <div class="container-fluid display-table">
        <div class="row display-table-row">
            <!--===================================SIDE MENU====================================-->
            <div class="col-md-2 col-sm-1 hidden-xs display-table-cell valign-top " id="side-menu">
                <a href="/Home/Index" style="background-color:#910B0B;"><img src="~/Media Content/Accenture-red-arrow-logo -white - Copy.png" style="height: 35px; width: 120.703px; padding-left:0px;" class="hidden-xs hidden-sm" /></a>

                <!--<h1 class="hidden-xs hidden-sm">Navigation</h1>-->
                <!--=============PROFILE IMAGE======================-->

                <div class="avatar-block hidden-sm hidden-xs">

                    <img src="~/Media Content/asong.jpeg" class="avatar img-responsive" id="avatar-img">
                    <div class="user-details">
                        <span class="header-name">Asongezwa</span>
                        <br />
                        <span class="header-surname"><strong>Sitwebile</strong></span>
                        <br />
                        <span style="color:#EEA3A5; font-size: 11px;">University of the Western Cape</span>
                        <br />
                        <span style="color:#EEA3A5; "><i class="fa fa-circle text-success"></i> Online</span>
                    </div>


                </div>
                <!--================================================-->



                <ul>
                    <li class="link active">
                        <a href="/Dashboard/Dashboard">
                            <span class="glyphicon glyphicon-th" aria-hidden="true"></span>
                            <span class="hidden-sm hidden-xs">Dashboard</span>
                        </a>
                    </li>


                    <li class="link">
                        <a href="/Dashboard/DashChat">
                            <span class="fa fa-comment pull-left" aria-hidden="true"></span>
                            <span class="label label-warning pull-right hidden-xs hidden-sm">5</span>
                            <span class="hidden-xs hidden-sm">Chats</span>

                        </a>
                    </li>

                    <li class="link">
                        <a href="forum.html">
                            <span class="glyphicon glyphicon-comment" aria-hidden="true"></span>
                            <span class="label label-warning pull-right hidden-xs hidden-sm">8</span>
                            <span class="hidden-sm hidden-xs">Forum</span>
                        </a>
                    </li>

                    <li class="link">
                        <a href="#collapse-comments" data-toggle="collapse" aria-controls="collapse-comments">
                            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                            <span class="hidden-xs hidden-sm">Comments</span>
                        </a>
                        <ul class="collapse collapseable" id="collapse-comments">
                            <li>
                                <a href="approved.html">
                                    Approved
                                    <span class="label label-success pull-right hidden-xs hidden-sm">10</span>
                                </a>
                            </li>

                            <li>
                                <a href="unapproved.html">
                                    Unapproved
                                    <span class="label label-warning pull-right hidden-xs hidden-sm">10</span>
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li class="link">
                        <a href="/Calendar/Dash_Calendar">

                            <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span>
                            <span class="hidden-xs hidden-sm">Calender</span>
                        </a>
                    </li>

                    <li class="link">
                        <a href="feedback.html">
                            <span class="glyphicon glyphicon-flag" aria-hidden="true"></span>
                            <span class="hidden-xs hidden-sm">Feedback</span>
                        </a>
                    </li>

                    <li class="link">
                        <a href="/Dashboard/DashFileUpload">
                            <span class="glyphicon glyphicon-upload" aria-hidden="true"></span>
                            <span class="hidden-xs hidden-sm">Upload Files</span>
                        </a>
                    </li>

                    <li class="link settings-btn">
                        <a href="settings.html">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                            <span class="hidden-xs hidden-sm">Settings</span>
                        </a>

                    </li>
                </ul>
            </div>
            <!--=============================END SIDE MENU=====================================-->
            <!--=============================MAIN CONTENT====================================-->
            <!--==============NAVBAR-DASH=================-->
            <div class="col-md-10 col-sm-11 box display-table-cell valign-top pull-right">
                <div class="row">
                    <header id="nav-header" class="clearfix">
                        <div class="col-md-5">
                            <nav class="navbar-default pull-left">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#side-menu">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </nav>
                            <input type="text" class="hidden-sm hidden-xs" id="header-search-field" placeholder="Search for something..." />
                        </div>
                        <div class="col-md-7">
                            <ul class="pull-right">
                                <li id="welcome" class="hidden-xs">Welcome to your Dashboard</li>
                                <li class="fixed-width">
                                    <a href="#">
                                        <span class="glyphicon glyphicon-bell" aria-hidden="true"></span>
                                        <span class=" label label-warning">3</span>
                                    </a>
                                </li>
                                <li class="fixed-width">
                                    <a href="#">
                                        <span class="glyphicon glyphicon-envelope" aria-hidden="true"></span>
                                        <span class=" label label-message">3</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" class="logout">
                                        <span class="glyphicon glyphicon-log-out" aria-hidden="true"> </span>
                                        Logout
                                    </a>
                                </li>

                            </ul>
                        </div>
                    </header>
                </div>
                <!--=========END NAVBAR DASH========-->
                <div class="row">
                    <footer id="admin-footer" class="clearfix">
                        <div class="pull-left"><b>Copyright</b>&copy; 2017</div>
                        <div class="pull-right">admin system</div>
                    </footer>
                </div>
            </div>
        </div>
    </div>






    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
    <script>
       
            $(document).ready(function () {
                $('[data-toggle="offcanvas"]').click(function () {
                    $('#side-menu').toggleClass('hidden-xs');
                });
            });
    </script>
</body>
</html>
